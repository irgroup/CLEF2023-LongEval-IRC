research goal:
  venue:
    name: CLEF
    year: 2023
implementation:
  cmd: python -m src.create_index --index WT & python -m src.create_index --index ST & python -m src.create_index --index LT && python -m systems.BM25 --index WT && python -m systems.BM25 --index ST && python -m systems.BM25 --index LT
  source:
    repository: https://github.com/irgroup/CLEF2023-LongEval-IRC.git
    commit: cbcc0e3e970ff0d906615e3b898930c0e2ae678d
    'lang:':
    - Text
    - Markdown
    - HyPhy
    - YAML
    - Jupyter Notebook
    - Python
    - JSON
    - CSV
    - INI
method:
  automatic: true
  indexing:
    method: pyterrier.TRECCollectionIndexer
    stemmer: PorterStemmer
    stopwords: removed
  retrieval:
    '1':
      name: bm25
      method: org.terrier.matching.models.BM25
      k_1: '1.2'
      k_3: '8'
      b: '0.75'
    '2':
      name: Bo1 query expansion
      method: pyterrier.rewrite.Bo1QueryExpansion
      fb_terms: '10'
      fb_docs: '3'
      reranks: bm25
    '3':
      name: XSqrA_M
      method: org.terrier.matching.models.XSqr_A_M
    '4':
      name: PL2
      method: org.terrier.matching.models.PL2
    '5':
      name: Reciprocal Rank Fusion (RRF)
      method: ranx.fusion.rrf
      min_k: '10'
      max_k: '100'
      step: '10'
      fuses:
      - RM3 query expansion
      - XSqrA_M
      - PL2
